{
  "name": "portfolio-iac-cdk",
  "version": "0.1.0",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:smoke": "jest --testMatch='**/*.smoke.test.ts'",
    "test:production": "jest --testMatch='**/*.production.test.ts'",
    "cdk": "cdk",
    "validate:dev": "echo 'Validating Dev environment'",
    "check:production-readiness": "node scripts/production-checks.js",
    "monitor:production": "node scripts/monitor-production.js",
    "lint": "eslint . --ext .ts",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "bootstrap": "./scripts/bootstrap-accounts.sh"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-cdk": "2.1021.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.205.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
