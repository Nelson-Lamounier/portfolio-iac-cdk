# CDK Bootstrap Environment Configuration Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# AWS Account IDs
PIPELINE_ACCOUNT_ID=123456789012
DEV_ACCOUNT_ID=111122223333
TEST_ACCOUNT_ID=444455556666
PROD_ACCOUNT_ID=999999999999

# AWS Region
AWS_REGION=us-east-1

# GitHub Repository (format: username/repository-name)
GITHUB_REPOSITORY=your-username/your-repository-name
GITHUB_OWNER=your-username
GITHUB_REPO=your-repository-name
GITHUB_BRANCH=main

# Domain Configuration
DOMAIN_NAME=your-domain.com

# CDK Configuration
CDK_VERSION=2.x
CDK_QUALIFIER=hnb659fds

# Optional: AWS Profile Names (if using named profiles)
AWS_PROFILE_PIPELINE=pipeline-account
AWS_PROFILE_DEV=dev-account
AWS_PROFILE_TEST=test-account
AWS_PROFILE_PROD=prod-account

# Bootstrap Configuration
FORCE_BOOTSTRAP=false
SKIP_VALIDATION=false

# Logging Configuration
LOG_LEVEL=INFO
DEBUG_MODE=false